<div class="card-home">
  <div>
    <img src="https://source.unsplash.com/random/200x100?charity,event?sig=<%= event.id %>" alt="" width="100%" height="33%" >
  </div>
  <div class=card-text>
    <h4 class="card-title"><%= event.event_name %></h4>
    <p><%= event.address %></p>
    <p><%= event.date %></p>
    <p><%= event.start_time %></p>
    <p><%= event.duration %></p>
  </div>
</div>
